{
    "reportName": "setup_c2_excavator2_byNexus_filt",
    "saveReport": true,
    "txt_label": "set up ExCavator2 CNVs filtered",
    "toPrint": true,

    "load_files": true,
    "fext": ".excavatorCNV.CNVannotated.txt",

    "editWith": "ExCavator2",
    "withFilter": true,
    "withProcess": false,

    "filt_kwargs": {
        "choose_col": "ProbCall",
        "choose_thres": 0.95
    },
    "edit_kwargs": {
        "keep_columns": "Chromosome,Start,End,Call,ProbCall,Gene_Symbol",
        "function_dict": {
            "Homozygous Copy Loss": -4,
            "CN Loss": -2,
            "CN Gain": 2,
            "High Copy Gain": 4,
            "LOH": -1
        },
        "mergeHow":"maxAll",
        "removeLOH":false
    },

    "comment": "#",
    "names": null,
    "split_patID": "_",
    "chr_col": "chr_int",
    "gene_id_col": "gene",
    "remove_patients": "",

    "plot_kwargs": {
        "cmap_custom": null,
        "vmin": -2,
        "vmax": 2,
        "highRes": false
    },
    
    "input_directory": "input,headneck,cohort2",
    "oncoscan_directory": "excavator2_byNexus",
    "oncoscan_files": "",

    "sample_info_read_csv_kwargs":{
        "sep": "\t",
        "header": 0,
        "index_col": 0
    },
    "sample_info_fname": "20180208_Oliver,clinical_data.csv",
    "sample_info_table_index_colname": "patient",
    "sample_info_table_sortLabels": "hasRelapsed",
    
    "output_directory": "output,headneck",
    "DEBUG": true
}